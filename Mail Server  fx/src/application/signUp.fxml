<AnchorPane prefHeight="600.0" prefWidth="800.0" styleClass="bodybg2" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SignUpController">
   <children>
      <AnchorPane layoutX="112.0" layoutY="79.0" prefHeight="427.0" prefWidth="568.0" styleClass="bodybg3">
         <children>
            <TextField fx:id="firstnameField" layoutX="309.0" layoutY="92.0" prefHeight="39.0" prefWidth="187.0" promptText="First Name" />
            <Button layoutX="225.0" layoutY="298.0" mnemonicParsing="false" onAction="#SignUp" prefHeight="39.0" prefWidth="98.0" text="submit" />
            <ComboBox fx:id="gender" layoutX="65.0" layoutY="222.0" prefHeight="39.0" prefWidth="187.0" promptText="Gender" />
